<!DOCTYPE html>
<html lang="en">

<%- include('../include/head.ejs') %>

<body>
    <%- include('../include/navbar.ejs') %>
    <% if (orders.length <= 0) {%>
    <h1>Nothing There</h1>
    <%} else {%> 
        <ul>
            <% orders.forEach(order =>  {%>
                <li>
                    <h1># <%= order._id%></h1>
                    <ul>
                        <% order.products.forEach(item => {%>

                        <li><%= item.product.title%>(<%= item.quantity %>)</li>
                        <%})%>
                    </ul>
                </li>
            <%})%>
        </ul>
    <% }%>